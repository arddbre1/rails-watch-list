<h1>Add a movie list📽️🎬</h1>
<div class="container">
<% @lists.each do |list| %>
<ul>
  <li>
  <strong><%= link_to list.name, list_path(list) %></strong>
  <%= link_to "❌delete", list_path(list), data:{turbo_method: :delete}, class: "btn btn-warning" %>
  </li>
  </ul>
<% end %>
</div>

<p><%= link_to "+Add a movie list",  new_list_path(@list), class: "btn btn-primary" %></p>
